generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  email     String   @unique
  image     String?
  name      String
  provider  String   @default("https://placeholderimage.com")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
